  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "directory": "dist"
  },
  "author": {
    "name": "Andika Candra"
  },
  "esm": {
    "cache": false
  },
  "license": "Apache-2.0",
  "repository": "git://github.com/aksarakan/setting.git",
  "dependencies": {
    "@axiona/string": "^0.18.0",
